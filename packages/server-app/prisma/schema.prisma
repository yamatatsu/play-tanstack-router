generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model PlcData {
  timestamp DateTime
  dataName  String
  value     Float

  @@id([timestamp, dataName])
}
